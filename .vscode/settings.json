{
    "oracledevtools.bookmarkFileFolder": "C:\\Users\\rodri\\Oracle\\oracle.oracledevtools",
    "oracledevtools.download.otherFolder": "C:\\Users\\rodri\\downloads",
    "oracledevtools.connectionConfiguration.configFilesFolder": "C:\\Users\\rodri\\Oracle\\network\\admin",
    "oracledevtools.connectionConfiguration.walletFileFolder": "C:\\Users\\rodri\\Oracle\\network\\admin",
    "[oraclesql]": {
      "editor.suggest.showSnippets": true,
      "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
      }
    },
    //"workbench.colorTheme": "Winter is Coming (Light)",
    "window.zoomLevel": 2,
    "terminal.integrated.fontFamily": "'Fira Code', monospace",
    "editor.wordWrap": "on",
  
    "editor.formatOnSave": true, // Formata o código sempre que salvamos
    "editor.fontFamily": "Source Code Pro", // Tamanho da Fonte
    "workbench.colorTheme": "Aura Soft Dark", // Tema
    "workbench.iconTheme": "vscode-icons", // Pacote de Ícones
    "workbench.startupEditor": "newUntitledFile", // Abre sempre um arquivo em branco
    "editor.minimap.enabled": false, // Desabilita o Mini Map
    "workbench.panel.defaultLocation": "right", // Deixa o terminal a direita
    "terminal.integrated.cursorStyle": "underline", // Tamanho da fonte do terminal
    "git.enableSmartCommit": true, // Habilita Smart Commit do GIT
    "git.autofetch": true,
    "workbench.editorAssociations": {
      "*.db": "sqlite-viewer.view"
    },
    "explorer.fileNesting.patterns": {
      "*.ts": "${capture}.js",
      "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
      "*.jsx": "${capture}.js",
      "*.tsx": "${capture}.ts",
      "tsconfig.json": "tsconfig.*.json",
      "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
      "*.fs": "${basename}.fsi, ${basename}.fs.js, ${basename}.fs.js.map",
      "*.fsl": "${basename}.fsi, ${basename}.fs",
      "*.fsy": "${basename}.fsi, ${basename}.fs",
      "*.sqlite": "${capture}.${extname}-*",
      "*.db": "${capture}.${extname}-*",
      "*.sqlite3": "${capture}.${extname}-*",
      "*.db3": "${capture}.${extname}-*",
      "*.sdb": "${capture}.${extname}-*",
      "*.s3db": "${capture}.${extname}-*"
    },
    "explorer.confirmDragAndDrop": false,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontSize": 15,
    "FSharp.suggestGitignore": false,
    "editor.defaultFormatter": "ms-dotnettools.csharp" // Habilita Auto Fetch do GIT
  }
  